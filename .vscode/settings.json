{
  "files.exclude": {
    "**/*.map": { "when": "$(basename).map" },
    "**/*.js": { "when": "$(basename).ts" },
    "**/*.mjs": { "when": "$(basename).ts" }
  },
  "search.exclude": {
    "**/dist": true
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "cSpell.words": [
    "Buggi"
  ]
}
